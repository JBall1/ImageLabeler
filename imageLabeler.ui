<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="main_window">
    <property name="height">400</property>
    <property name="width">800</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="fm_main">
        <property name="height">400</property>
        <property name="width">800</property>
        <layout manager="pack">
          <property name="propagate">True</property>
          <property name="side">right</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="selectDir">
        <property name="text" translatable="yes">Select Directory</property>
        <property name="textvariable">string:selectedDir</property>
        <layout manager="pack">
          <property name="padx">5</property>
          <property name="pady">5</property>
          <property name="propagate">True</property>
          <property name="side">left</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Separator" id="separator44">
        <property name="orient">horizontal</property>
        <layout manager="pack">
          <property name="fill">y</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
